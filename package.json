{
    "name": "lugo4ts",
    "version": "1.0.0",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "dev": "tsx src/index.ts",
        "build": "tsc",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "proto:generate": "npx grpc_tools_node_protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=src/generated2 --grpc_out=src/generated2 -I protos protos/*.proto",
        "proto:generate2": "protoc --plugin=protoc-gen-ts=npx --ts_out=grpc_js:src/generated2 --js_out=import_style=commonjs,binary:src/generated2 --grpc_out=grpc_js:src/generated2 -I protos protos/*.proto",
        "proto:generate3": "protoc --ts_out=import_style=commonjs,binary:src/generated2 --grpc_out=src/generated2 --plugin=protoc-gen-grpc=`which grpc_tools_node_protoc_plugin` -I protos protos/*.proto",
        "proto:generate4": "npx protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/generated2 -I protos protos/*.proto",
        "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
        "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json}\""
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
    "dependencies": {
        "@bufbuild/protobuf": "^2.5.1",
        "@connectrpc/connect": "^2.0.2",
        "@connectrpc/connect-node": "^2.0.2",
        "@grpc/grpc-js": "^1.13.4",
        "@grpc/proto-loader": "^0.7.15",
        "@protobuf-ts/grpc-transport": "^2.11.0"
    },
    "devDependencies": {
        "@bufbuild/buf": "^1.54.0",
        "@bufbuild/protoc-gen-es": "^2.5.1",
        "@eslint/js": "^9.28.0",
        "@protobuf-ts/plugin": "^2.11.0",
        "@protobuf-ts/protoc": "^2.11.0",
        "@protobuf-ts/runtime": "^2.11.0",
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "@types/node": "^22.15.29",
        "@typescript-eslint/eslint-plugin": "^8.33.1",
        "@typescript-eslint/parser": "^8.33.1",
        "@vitest/coverage-istanbul": "^3.1.4",
        "eslint": "^9.28.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-import": "^2.31.0",
        "grpc_tools_node_protoc_ts": "^5.3.3",
        "grpc-tools": "^1.13.0",
        "prettier": "^3.5.3",
        "rxjs": "^7.8.2",
        "ts-proto": "^2.7.2",
        "ts-protoc-gen": "^0.15.1-pre.a71b34e",
        "tsx": "^4.19.4",
        "typescript": "^5.8.3",
        "vitest": "^3.1.4"
    }
}
